
.finish-mask{
	width: 100%;
	height:100%;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 8;
	background: transparent;
}
.finish-tip{
	width: 280rpx;
	height: 280rpx;
	border-radius: 10rpx;
	text-align: center;
	color: #fff;
	position: absolute;
	z-index: 10;
	left: 50%;
	top: 50%;
	transform: translate(-50%,-50%);
	background: rgba(0, 0, 0, 0.5)
}
.finish-tip image{
	width: 100rpx;
	height: 70rpx;
	margin-top: 60rpx;
	margin-bottom: 30rpx;
	vertical-align: middle;
}
.finish-tip text{
	display: block;
	font-size: 26rpx;
}