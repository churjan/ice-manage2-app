/* pages/allIce/allIce.wxss */
.ice-list{
    height: 120rpx;
    line-height: 120rpx;
    border-top: 1rpx solid #e5e5e5;
    padding: 0 30rpx;
    display: flex;
    justify-content: space-between;
}
.ice-list .left{
    font-size: 32rpx;
    line-height: 1;
    color: #333333;
    display: flex;
    justify-content: flex-start;
}
.ice-list .left .left-point{
    width: 50rpx;
    display: flex;
    flex-flow: column wrap;
    justify-content: center;
}
.ice-list .left .left-shop{
    display: flex;
    flex-flow: column wrap;
    justify-content: center;
}
.ice-list .left .left-shop .name{
    max-width: 430rpx;
}
.ice-list .left .left-shop .num{
    color: #ababab;
    font-size: 26rpx;
    margin-top: 16rpx;
}
.ice-list .right{
    color: #f7941e;
    font-size: 32rpx;
}
.ice-list .right text:nth-of-type(1){
    padding-right: 30rpx;
    border-right: 1rpx solid #e5e5e5;
}
.ice-list .right text:nth-of-type(2){
    padding-left: 30rpx;
}
.good-point {
    width: 20rpx;
    height: 20rpx;
    border-radius: 20rpx;
    background-color: #68bd68;
}
.bad-point{
    width: 20rpx;
    height: 20rpx;
    border-radius: 20rpx;
    background-color: #d33642;
}
.shop-detail{
    width: 100%;
    height: 100vh;
    position: fixed;
    top: 0;
    background-color: rgba(0, 0, 0, 0.3);
    z-index: 999;
}
.shop-body{
    background-color: #f8f8f8;
    font-size: 32rpx;
    color: #333;
    position: absolute;
    bottom: 0;
    width: 100%;
}
.shop-header, .shop-main, .shop-bottom{
    background-color: #fff;
    margin-bottom: 20rpx;
}
.shop-header-item1, .shop-header-item2{
    border-bottom: 1rpx solid #e5e5e5;
    padding: 20rpx 50rpx;
}
.shop-header-item1{
    height: 50rpx;
    line-height: 50rpx;
    display: flex;
    justify-content: space-between;
    font-weight: bold;
}
.shop-header-item1 text:nth-of-type(2){
    font-size: 28rpx;
    color: #f7941e;
    font-weight: lighter;
}
.shop-header-item2{
    position: relative;
    min-height: 90rpx;
    font-weight: bold;
}
.shop-header-item2 text:nth-of-type(2){
    font-size: 26rpx;
    color: #ababab;
    font-weight: lighter;
	display: block;
	padding: 0;
}

.shop-main{
    border-top: 1rpx solid #e5e5e5;
    border-bottom: 1rpx solid #e5e5e5;
    padding: 0 50rpx;
}
.shop-main-item0, .shop-main-item1, .shop-main-item2, .shop-main-item3{
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 1rpx solid #e5e5e5;
    padding-left: 55rpx;
    display: flex;
    justify-content: space-between;
    position: relative;
}
.shop-main-item0 text{

}
.good-text{
	color:#68bd68;
}
.warn-text{
	color: #d33642;
}
.point-wrap{
    display: flex;
    justify-content: center;
    margin-top:40rpx;
}
.shop-main-item0 .point-wrap,.shop-main-item1 .point-wrap{
	margin-top: 0;
}
.point-wrap text{
    margin-left:  30rpx;
}
.shop-main-item3{
    border-bottom: none;
}
.shop-main-item0::before,.shop-main-item1::before,.shop-main-item2::before,.shop-main-item3::before{
    content: '';
    width: 24rpx;
    height: 30rpx;
    background-size: cover;
    position: absolute;
    left: 6rpx;
    top: 0;
    bottom: 0;
    margin: auto;
}
.shop-main-item0::before{
	width: 30rpx;
    height: 25rpx;
    background-image: url(data:image/png;base64,);
}
.shop-main-item1::before{
    background-image: url(data:image/png;base64,);
}
.shop-main-item2::before{
    background-image: url(data:image/png;base64,);
}
.shop-main-item3::before{
    background-image: url(data:image/png;base64,);
}
.shop-main-item3 image{
	display: inline-block;
	width: 40rpx;
	height: 30rpx;
	margin-top: 35rpx;
}
.shop-bottom{
    display: flex;
    justify-content: space-between;
    color: #f7941e;
    border-top: 1rpx solid #e5e5e5;
    margin: 0;
}
.shop-bottom view{
    width: 50%;
    text-align: center;
    height: 103rpx;
    line-height: 103rpx;
}
.shop-bottom view:nth-of-type(1){
    border-right: 1rpx solid #e5e5e5;
}
.reset{
    position: absolute;
    display: flex;
    justify-content: center;
    align-items: center;
    right: 28rpx;
    top: 0;
    font-size: 28rpx;
    height: 50rpx;
    padding: 0 20rpx;
    border: 2rpx solid #ddd;
    color: #aaa;
    border-radius: 27rpx/24rpx;
    margin-top: 18rpx;
    font-weight: normal;
    box-sizing: border-box;
}