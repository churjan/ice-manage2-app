/* dispatching/pages/distributionList/distributionList.wxss */
.search-container{
    background: #fff;
    z-index: 999;
}
.area-list{
    color: #555555;
}
.area{
    padding: 0 50rpx;
    font-weight: bold;
}
.city{
    font-weight: bold;
    border-top: 1rpx solid #e5e5e5;
    border-bottom: 1rpx solid #e5e5e5;
    padding: 0 50rpx;
}
.three-level view{
    padding: 0 50rpx;
}
.area-list view{
    line-height: 80rpx;
    position: relative;
}
.area{
    background: #f4f4f4;
}
.select-item{
    color: #f7941e;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.select-item::before{
    content: '';
    background: url(data:image/png;base64,) center/26rpx 20rpx no-repeat;
    width: 26rpx;
    height: 20rpx;
    position: absolute;
    right: 50rpx;
}