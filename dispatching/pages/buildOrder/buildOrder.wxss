/* dispatching/pages/buildOrder/buildOrder.wxss */
.main{
    background: #fff;
}
.add-button{
    color: #f7941e;
    font-size: 32rpx;
    line-height:ã€€100rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}
.add-button::before{
    content: '+';
    font-size: 50rpx;
    padding-right: 10rpx;
}
.item-list{
    padding: 0 50rpx;

}   
.item{
    line-height: 100rpx;
    display: flex;
    justify-content: space-between;
    width: 100%;
    color: #333333;
    font-size: 30rpx;
    border-top: 1rpx solid #d9d9d9;
}
.shop-name{
    width: 565rpx;
    font-size: 30rpx;
}
.address{
    display: flex;
    position: relative;
    padding: 0 50rpx;
    justify-content: center;
    flex-flow: column;
    height: 146rpx;
    padding-right: 50rpx;
}
.address view{
    line-height: 50rpx;
}
.address-ico{
    display: flex;
    align-items: flex-start;
}
.address-ico::before{
    content: '';
    background: url(data:image/png;base64,) center/21rpx 35rpx no-repeat;
    width: 21rpx;
    height: 35rpx;
    right: 50rpx;
    position: absolute;
}
.un-address{
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 146rpx;
    border-bottom: 1rpx solid #d9d9d9;
    padding: 0 50rpx;
}

.close{
    width: 28rpx;
    height: 28rpx;
}